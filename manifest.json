{
	"manifest_version": 2,
	"name": "more-worked",
	"version": "1.0",
	"description": "An extension that changes the phrase \"open-sourced\" to \"more-worked\"",
	"icons": { "16": "images/morework16.png", "48": "images/morework48.png", "128": "images/morework128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"], 
			"js": ["moreworked.js"],
			"run_at": "document_end"  
		}
	]
}
